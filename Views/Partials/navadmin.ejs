        <% if(user === "") { %>
            <a href='/user/login'>Login</a>
            <% } else { %>
                <a href='/user/profile'>Profile</a>
                <a href='/signout'>Logout</a>
                <div class="user">
                    Welcome
                    <%= user.username %>
                </div>
                <% } %>
    </ul>
    <ul>
        <div class="logo">
            <a href="/admin-homepage"><img src="/images/TopNav/LogoNew.png"></a>
        </div>
        <div class="topnav" id="myTopnav">
            <a href="/admin-user-management">User Management</a>
            <a href="/admin-product-management">Product Control</a>
        </div>
        <div class="lowerbar"></div>
    </ul>
